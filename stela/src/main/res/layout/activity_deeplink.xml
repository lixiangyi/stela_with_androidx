<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_deeplink"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
    </androidx.recyclerview.widget.RecyclerView>

    <ImageView
        android:id="@+id/iv_close"
        android:layout_width="@dimen/stela_app_dp_32"
        android:layout_height="@dimen/stela_app_dp_32"
        android:layout_alignParentRight="true"
        android:src="@mipmap/deeplink_close"
        android:visibility="gone"
        android:layout_margin="@dimen/stela_app_dp_12"/>
    <ImageView
        android:id="@+id/iv_scroll"
        android:layout_width="@dimen/stela_app_dp_56"
        android:layout_height="@dimen/stela_app_dp_56"
        android:layout_centerInParent="true"
        android:src="@mipmap/deeplink_scroll"
        />
    <ImageView
        android:id="@+id/iv_sign_up"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:src="@mipmap/deeplink_signup"
        android:visibility="gone"
        android:layout_marginBottom="@dimen/stela_app_dp_52"/>



</RelativeLayout>